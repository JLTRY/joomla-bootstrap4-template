{
  "$schema": "https://developer.joomla.org/schemas/json-schema/web_assets.json",
  "name": "cassiopeia",
  "version": "4.0.0",
  "description": "bootstrap4 is the Joomla 4 site template",
  "license": "GPL-2.0+",
  "assets": [
   {
      "name": "bootstrap.bundle",
      "type": "script",
      "uri": "media/vendor/bootstrap/js/bootstrap.bundle.js",
	  "dependencies": [
        "core",
		"jquery"
	  ]
    },
    {
      "name": "template.bootstrap4",
      "type": "script",
      "dependencies": [
        "core",
		"bootstrap.bundle",
		"bootstrap.dropdown",
		"awesomplete"
      ],
      "uri": "user.js"
    },
    {
      "name": "template.bootstrap4.fontawesome",
      "type": "style",
      "uri": "https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.13.0/css/fontawesome.min.css"
    },
   {
      "name": "template.bootstrap4.fontawesome.solid",
      "type": "style",
      "uri": "https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.13.0/css/solid.css"
    },
    {
      "name": "template.bootstrap4.googlefont",
      "type": "style",
      "uri": "https://fonts.googleapis.com/css?family=Fira+Sans:400"
    },
    {
      "name": "template.bootstrap4.typography",
      "type": "style",
      "uri": "plugins/editors/jckeditor/typography/typography.css"
    },
    {
      "name": "template.bootstrap4.bootstrap",
      "type": "style",
      "uri": "bootstrap.min.css"
    },
    {
      "name": "template.bootstrap4.template",
      "type": "style",
      "uri": "template.css"
    },
    {
      "name": "template.bootstrap4.templatemod",
      "type": "style",
      "uri": "templatemod.css"
    },
	{
      "name": "template.bootstrap4.user",
      "type": "style",
      "uri": "user.css"
    },
    {
      "name": "template.bootstrap4.ltr",
      "type": "style",
      "dependencies": [ "awesomplete" , "template.bootstrap4.fontawesome", "template.bootstrap4.fontawesome.solid", "template.bootstrap4.typography", "template.bootstrap4.templatemod", "template.bootstrap4.user", "template.bootstrap4.bootstrap", "template.bootstrap4.template"]
    },
    {
      "name": "template.bootstrap4.rtl",
      "type": "style",
      "dependencies": [ "awesomplete", "template.bootstrap4.fontawesome", "template.bootstrap4.fontawesome.solid", "template.bootstrap4.typography", "template.bootstrap4.templatemod", "template.bootstrap4.user", "template.bootstrap4.bootstrap", "template.bootstrap4.template"]
    },
    {
      "name": "template.bootstrap4.ltr",
      "type": "preset",
      "dependencies": [
        "template.bootstrap4.ltr#style",
        "template.bootstrap4#script"
      ]
    },
    {
      "name": "template.bootstrap4.rtl",
      "type": "preset",
      "dependencies": [
        "template.bootstrap4.rtl#style",
        "template.bootstrap4#script"
      ]
    }
  ]
}
